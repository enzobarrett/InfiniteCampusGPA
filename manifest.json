{
  "manifest_version": 2,
  "name": "EyeSeeGrades",
  "description": "",
  "version": "0.0.0.1",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": ["bg.js"]
  },
  "content_scripts": [{
    "matches": ["https://bvsd.infinitecampus.org/campus/portal/*"],
    "all_frames": true,
    "js": ["inject.js"]
  }]
}
