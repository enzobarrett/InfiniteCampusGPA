{
  "manifest_version": 2,
  "name": "EyeSeeGrades",
  "description": "",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": ["bg.js"]
  },
  "content_scripts": [{
    "matches": ["https://bvsd.infinitecampus.org/campus/portal/*"],
    "all_frames": true,
    "js": ["inject.js"]
  }]
}
