{
  "manifest_version": 2,
  "name": "IC Grade Reader",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "persistent": true,
    "scripts": ["bg.js"]
  },

  "content_scripts": [{
    "matches": ["https://bvsd.infinitecampus.org/campus/portal/*"],
    "js": ["inject.js"]
  }]

}
